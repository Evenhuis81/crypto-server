{
    "name": "blank",
    "author": "D",
    "private": true,
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=16.0.0"
    },
    "scripts": {
        "dev": "npm run server",
        "prod": "npm run build && npm run server:prod",
        "build": "vite build",
        "server": "ts-node ./server",
        "server:prod": "cross-env NODE_ENV=production ts-node ./server",
        "prettier:check": "prettier --check \"./**/*.{js,ts,vue}\"",
        "prettier:write": "prettier --write \"./**/*.{js,ts,vue}\"",
        "typescript-check": "tsc --noEmit -p tsconfig.json",
        "vue-typescript-check": "vue-tsc -p tsconfig.json",
        "lint": "eslint \"./**/*.{js,vue,ts}\" --fix --cache",
        "lint:pipeline": "eslint \"./**/*.{js,vue,ts}\" --format=json -o eslint.out"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^3.0.3",
        "@vue/compiler-sfc": "^3.2.33",
        "@vue/server-renderer": "^3.2.33",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "express": "^4.18.1",
        "sirv": "^2.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4",
        "vite": "^3.0.9",
        "vite-plugin-ssr": "^0.4.54",
        "vue": "^3.2.33"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.12",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.8.0",
        "prettier": "2.8.1",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.13"
    }
}
